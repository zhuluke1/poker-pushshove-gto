{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
          "includeFiles": ["requirements.txt"],
          "buildCommand": "pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ]
  }
  