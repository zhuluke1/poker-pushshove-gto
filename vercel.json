{
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
          "buildCommand": "cp ../../requirements.txt . && pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ]
  }